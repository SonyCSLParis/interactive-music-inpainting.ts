{
  "name": "nonoto",
  "version": "0.1.1",
  "description": "Electron and web application for interactive music composition",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ./app/main.js",
    "link-osmd": "yarn link opensheetmusicdisplay",
    "build:web": "webpack --config webpack.web.js",
    "build:notono_web": "webpack --config webpack.notono_web.js",
    "build:notono_web_anonymous": "webpack --config webpack.notono_web_anonymous.js",
    "serve:web": "http-server -p 8080 -P http://localhost:5000 ./web/dist",
    "serve:web_anonymous": "http-server -p 8080 -P http://localhost:5000 ./web/dist_anonymous",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack"
  },
  "build": {
    "extraResources": [
      {
        "from": "./static/",
        "to": "static",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonyCSLParis/NONOTO.git"
  },
  "keywords": [
    "opensheetmusicdisplay",
    "vexflow",
    "deepbach"
  ],
  "author": "Théis Bazin (theis.bazin@sony.com), Hadjeres Gaëtan (gaetan.hadjeres@sony.com)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SonyCSLParis/NONOTO/issues"
  },
  "homepage": "https://github.com/opensheetmusicdisplay/webpack-usage-example#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@types/deep-equal": "^1.0.1",
    "@types/fs-extra": "^9.0.1",
    "@types/jquery": "^3.3.5",
    "@types/node": "12",
    "@types/simplebar": "^5.1.1",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.32",
    "bufferutil": "^4.0.1",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "electron": "^11",
    "electron-builder": "^22.0.0",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^4",
    "file-loader": "^6.0.0",
    "http-server": "^0.12.3",
    "jquery": "^3.5.1",
    "node-gyp": "^7.0.0",
    "sass": "^1.26.8",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "^4",
    "url-loader": "^4.1.0",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4",
    "webpack-json-loader": "^1.0.0"
  },
  "dependencies": {
    "@tonejs/midi": "^2.0.25",
    "@tonejs/piano": "^0.2.1",
    "abletonlink": "^0.1.1",
    "activity-detector": "^3.0.0",
    "ajv": "^7.0.3",
    "deep-equal": "^2.0.3",
    "es6-promise": "^4.2.4",
    "jquery-simple-upload": "^1.1.0",
    "jszip": "^3.2.2",
    "loglevel": "^1.6.1",
    "nexusui": "^2.1.4",
    "nipplejs": "^0.8.5",
    "opensheetmusicdisplay": "^0.8.7",
    "request": "^2.87.0",
    "require": "^2.4.20",
    "screenfull": "^5.0.2",
    "simplebar": "^5.2.0",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "source-map-support": "^0.5.6",
    "teoria": "^2.5.0",
    "tone": "^14.5.28",
    "trip.js": "^3.3.3",
    "webmidi": "^2.3.0",
    "webpack-sources": "^2.2.0",
    "wheelnav": "^1.7.1"
  },
  "resolutions": {
    "webpack-sources": "^1.0.1"
  }
}
