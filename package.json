{
  "name": "nonoto",
  "version": "0.1.1",
  "description": "Electron and web application for interactive music composition",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall-disabled": "electron-builder install-app-deps",
    "start": "electron ./app/main.js",
    "build:web": "webpack --config webpack.web.cjs",
    "build:notono_web": "webpack --config webpack.notono_web.cjs",
    "build:notono_web_anonymous": "webpack --config webpack.notono_web_anonymous.cjs",
    "serve:web": "http-server -p 8080 -P http://localhost:5000 ./web/dist",
    "serve:web-no_cache": "http-server -c-1 -p 8080 -P http://localhost:5000 ./web/dist",
    "serve:web_anonymous": "http-server -p 8080 -P http://localhost:5000 ./web/dist_anonymous",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "deploy:notono": "aws s3 sync ./web/dist/ s3://notono-static-website && aws cloudfront create-invalidation --distribution-id EM6BPT6R6J692 --paths '/*'"
  },
  "build": {
    "extraResources": [
      {
        "from": "./static/",
        "to": "static",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonyCSLParis/NONOTO.git"
  },
  "keywords": [
    "opensheetmusicdisplay",
    "vexflow",
    "deepbach"
  ],
  "author": "Théis Bazin (theis.bazin@sony.com), Hadjeres Gaëtan (gaetan.hadjeres@sony.com)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SonyCSLParis/NONOTO/issues"
  },
  "homepage": "https://github.com/SonyCSLParis/NONOTO",
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jquery": "^3.5.14",
    "@types/node": "12.20.50",
    "@types/simplebar": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
    "electron": "^11.5.0",
    "electron-builder": "^22.14.13",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-webpack-plugin": "^2.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4",
    "http-server": "^14.1.0",
    "node-gyp": "^7.0.0",
    "prettier": "2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^10.2.1",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^2.0.0",
    "typescript": "^4",
    "url-loader": "^4.1.1",
    "utf-8-validate": "^5.0.9",
    "webpack": "^4.46.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@tonaljs/tonal": "^4.6.5",
    "@tonejs/midi": "^2.0.28",
    "@tonejs/piano": "^0.2.1",
    "abletonlink": "^0.1.3",
    "audiokeys": "https://github.com/SonyCSLParis/AudioKeys",
    "chart.js": "^3.7.1",
    "deep-equal": "^2.0.5",
    "fs-extra": "^10.1.0",
    "idle-tracker": "^0.1.2",
    "jquery": "^3.6.0",
    "jquery-simple-upload": "^1.1.0",
    "jszip": "^3.9.1",
    "loglevel": "^1.8.0",
    "nexusui": "https://github.com/SonyCSLParis/nexusui#types",
    "opensheetmusicdisplay": "^1.5.0",
    "raphael": "^2.3.0",
    "request": "^2.88.2",
    "require": "^2.4.20",
    "screenfull": "^5.0.2",
    "shepherd.js": "^8.3.1",
    "simplebar": "^5.3.6",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "source-map-support": "^0.5.21",
    "tiny-typed-emitter": "^2.1.0",
    "tone": "latest",
    "typed-undo": "^2.3.0",
    "webmidi": "^3.0.19",
