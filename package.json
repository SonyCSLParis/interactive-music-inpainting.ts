{
  "name": "interactive-score-interface",
  "version": "0.0.1",
  "description": "Electron interactive application for the DeepBach generative model",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ./app/main.js",
    "webpack": "yarn link-osmd && yarn build-main && yarn build-renderer",
    "link-osmd": "yarn link opensheetmusicdisplay",
    "build-main": "webpack --config webpack.main.js",
    "build-renderer": "webpack --config webpack.renderer.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "yarn link-osmd && electron-webpack dev",
    "compile": "yarn link-osmd && electron-webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonyCSLParis/interactive-score-interface.git"
  },
  "keywords": [
    "opensheetmusicdisplay",
    "vexflow",
    "deepbach"
  ],
  "author": "Th√©is Bazin (theis.bazin@sony.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SonyCSLParis/interactive-score-interface/issues"
  },
  "homepage": "https://github.com/opensheetmusicdisplay/webpack-usage-example#readme",
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/electron": "^1.6.10",
    "@types/jquery": "^3.3.5",
    "@types/node": "^10.5.4",
    "awesome-typescript-loader": "^5.2.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.5",
    "electron-builder": "^20.26.1",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "typescript": "^3.0.1",
    "webpack": "^4",
    "webpack-json-loader": "^1.0.0"
  },
  "dependencies": {
    "abletonlink": "0.0.7",
    "ajv": "^6.5.2",
