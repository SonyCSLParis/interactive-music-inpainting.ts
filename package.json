{
  "name": "nonoto",
  "version": "0.2.0",
  "description": "Electron and web application for interactive music composition",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "serve:web-no_cache": "http-server -c-1 -p 8080 -P http://localhost:5000 ./packages/renderer/dist"
  },
  "old_scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall-disabled": "electron-builder install-app-deps",
    "start": "electron ./app/main.js",
    "build:web": "webpack --config webpack.web.cjs",
    "build:notono_web": "webpack --config webpack.notono_web.cjs",
    "build:notono_web_anonymous": "webpack --config webpack.notono_web_anonymous.cjs",
    "serve:web": "http-server -p 8080 -P http://localhost:5000 ./web/dist",
    "serve:web_anonymous": "http-server -p 8080 -P http://localhost:5000 ./web/dist_anonymous",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "deploy:notono": "aws s3 sync ./web/dist/ s3://notono-static-website && aws cloudfront create-invalidation --distribution-id EM6BPT6R6J692 --paths '/*'"
  },
  "build": {
    "extraResources": [
      {
        "from": "./static/",
        "to": "static",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonyCSLParis/NONOTO.git"
  },
  "keywords": [
    "opensheetmusicdisplay",
    "vexflow",
    "deepbach"
  ],
  "author": "Théis Bazin (theis.bazin@sony.com), Hadjeres Gaëtan (gaetan.hadjeres@sony.com)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SonyCSLParis/NONOTO/issues"
  },
  "homepage": "https://github.com/SonyCSLParis/NONOTO",
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jquery": "^3.5.14",
    "@types/node": "17.0.31",
    "@types/simplebar": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cross-env": "^7.0.3",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.9",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^14.1.0",
    "node-gyp": "^9.0.0",
    "prettier": "2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "utf-8-validate": "^5.0.9",
    "vite": "^2.9.8"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@tonaljs/tonal": "^4.6.5",
    "@tonejs/midi": "^2.0.28",
    "@tonejs/piano": "^0.2.1",
    "abletonlink": "^0.1.3",
    "audiokeys": "https://github.com/SonyCSLParis/AudioKeys",
    "chart.js": "^3.7.1",
    "deep-equal": "^2.0.5",
    "events": "^3.3.0",
    "fs-extra": "^10.1.0",
    "idle-tracker": "^0.1.2",
    "jquery": "^3.6.0",
    "loglevel": "^1.8.0",
    "nexusui": "https://github.com/SonyCSLParis/nexusui#types",
    "opensheetmusicdisplay": "^1.5.0",
    "screenfull": "^6.0.1",
    "shepherd.js": "^9.1.0",
    "simplebar": "^5.3.6",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "tiny-typed-emitter": "^2.1.0",
    "tone": "latest",
    "typed-undo": "^2.3.0",
    "webmidi": "^3.0.20",
    "wheelnav": "^1.7.1"
  },
  "packageManager": "yarn@3.1.1"
}
